<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
    <script src="/socket.io/socket.io.js"></script>
</head>

<body>
    <h1>Dashboard</h1>

    <script>
        const socket = io();

        socket.on('newNotification', (data) => {
            alert(data.message);
        });


    </script>
</body>

</html>